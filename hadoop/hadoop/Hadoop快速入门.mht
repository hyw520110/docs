function getDirectoriesInURL()=0A=
{=0A=
	var trail =3D document.location.pathname.split( PATH_SEPARATOR );=0A=
=0A=
	// check whether last section is a file or a directory=0A=
	var lastcrumb =3D trail[trail.length-1];=0A=
	for( var i =3D 0; i < FILE_EXTENSIONS.length; i++ )=0A=
	{=0A=
		if( lastcrumb.indexOf( FILE_EXTENSIONS[i] ) )=0A=
		{=0A=
